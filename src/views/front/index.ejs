                        <div class="tz-grid-post-0">
                            <div class="tz-featured-thumb">
                                <a href="/news/details/<%= slideBlock[0].slug %>">
                                    <img src="/<%= slideBlock[0].image %>" alt="/<%= slideBlock[0].image %>">
                                </a>
                            </div>
                            <div class="tz-featured-info">
                                <a class="glod cat_name" href="/category/<%= slideBlock[0].cat[0].slug %>"><%= slideBlock[0].cat[0].title %></a>
                                <h3><a href="/news/details/<%= slideBlock[0].slug %>"> <%= slideBlock[0].title %> </a></h3>
                                <span class="tz-featured-meta"> <%= moment(slideBlock[0].createdAt).format('Do MMMM, YYYY') %> </span>
                            </div>
                        </div>
                        <!--End Item-->
                        <% } %>
                        <!--Item One-->
                        <% Object.keys(slideBlock).forEach(function(prop) { if(prop!=0){ %>
                        <div class="tz-grid-post-1">
                            <div class="tz-featured-thumb">
                                <a href="/news/details/<%= slideBlock[prop].slug %>">
                                    <img src="/<%= slideBlock[prop].image %>" alt="<%= slideBlock[prop].title %>">
                                </a>
                            </div>
